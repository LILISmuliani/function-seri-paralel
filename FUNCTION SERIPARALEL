#include <iostream>
using namespace std;
int main (){
	
	float R1, R2, R3;
	cout << " Menghitung hammbatan seri dan hambatan paralel " << endl;
	
		cout << " Masukkan R1 : ";
		cin >> R1;
		cout << " Masukkan R2 : ";
		cin >> R2;
		cout << " Masukkan R3 : ";
		cin >> R3;
		
		float HS;
		HS = R1 + R2 + R3;
		float HP;
		HP = 1/R1 + 1/R2 + 1/R3;
		
		cout << " Hambatan Seri = " << HS << endl;
		cout << " Hambatan Paralel = " << HP << endl;
		
		return 0;
}
